{
  "name": "nucleate",
  "version": "0.0.1",
  "description": "A static site generator powered by React, Redux, and webpack",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "develop": "babel src --watch --out-dir lib",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliottsj/nucleate.git"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Spencer Elliott <me@elliottsj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elliottsj/nucleate/issues"
  },
  "homepage": "https://github.com/elliottsj/nucleate#readme",
  "peerDependencies": {
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^5.0.0-beta4",
    "babel-jest": "^5.3.0",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "eslint": "^1.10.1",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-plugin-react": "^3.10.0",
    "eslint-plugin-standard": "^1.3.1",
    "jest-cli": "^0.8.0"
  },
  "dependencies": {
    "file-loader": "^0.8.5",
    "front-matter-loader": "^0.1.2",
    "history": "^1.13.1",
    "hoist-non-react-statics": "^1.0.3",
    "html-loader": "^0.3.0",
    "invariant": "^2.1.2",
    "json-loader": "^0.5.3",
    "markdown-it-loader": "^0.2.0",
    "nucleate-webpack-plugin": "^0.0.1",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "redux-router": "^1.0.0-beta4",
    "redux-thunk": "^1.0.0",
    "reselect": "^2.0.1",
    "wu": "^2.1.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/wu"
    ]
  }
}
