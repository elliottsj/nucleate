{
  "name": "nucleate",
  "version": "0.1.0",
  "description": "A static site generator powered by React, Redux, and webpack",
  "main": "lib/index.js",
  "bin": "lib/bin/nucleate.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "develop": "babel src --watch --out-dir lib",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliottsj/nucleate.git"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Spencer Elliott <me@elliottsj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elliottsj/nucleate/issues"
  },
  "homepage": "https://github.com/elliottsj/nucleate#readme",
  "peerDependencies": {
    "react": "^0.14.1",
    "react-dom": "^0.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^5.0.0-beta4",
    "babel-jest": "^6.0.1",
    "babel-polyfill": "^6.2.0",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.10.0",
    "jest-cli": "^0.8.0"
  },
  "dependencies": {
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "combine-loader": "^0.1.0",
    "commander": "^2.9.0",
    "eval": "^0.1.1",
    "front-matter-loader": "^0.1.2",
    "history": "^2.0.0-rc2",
    "hoist-non-react-statics": "^1.0.3",
    "html-loader": "^0.4.0",
    "invariant": "^2.1.2",
    "json-loader": "^0.5.3",
    "loader-utils": "^0.2.12",
    "markdown-it-loader": "^0.2.0",
    "nucleate-webpack-plugin": "^0.0.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0-rc5",
    "rx-lite": "^4.0.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0",
    "wu": "^2.1.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules"
    ]
  }
}
